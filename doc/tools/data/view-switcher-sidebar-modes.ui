<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <object class="GtkBox" id="widget">
    <property name="spacing">12</property>
    <child>
      <object class="AdwViewSwitcherSidebar">
        <property name="stack">stack</property>
        <property name="width-request">200</property>
        <style>
          <class name="docs-background"/>
        </style>
      </object>
    </child>
    <child>
      <object class="AdwViewSwitcherSidebar">
        <property name="stack">stack</property>
        <property name="mode">page</property>
        <property name="width-request">360</property>
        <style>
          <class name="docs-background"/>
        </style>
      </object>
    </child>
  </object>
  <object class="AdwViewStack" id="stack">
    <child>
      <object class="AdwViewStackPage">
        <property name="title">Page 1</property>
        <property name="icon-name">applications-system-symbolic</property>
        <property name="child">
          <object class="AdwBin"/>
        </property>
      </object>
    </child>
    <child>
      <object class="AdwViewStackPage">
        <property name="title">Page 2</property>
        <property name="icon-name">applications-system-symbolic</property>
        <property name="child">
          <object class="AdwBin"/>
        </property>
      </object>
    </child>
    <child>
      <object class="AdwViewStackPage">
        <property name="title">Page 3</property>
        <property name="icon-name">applications-system-symbolic</property>
        <property name="badge-number">3</property>
        <property name="starts-section">True</property>
        <property name="section-title">Badges</property>
        <property name="child">
          <object class="AdwBin"/>
        </property>
      </object>
    </child>
    <child>
      <object class="AdwViewStackPage">
        <property name="title">Page 4</property>
        <property name="icon-name">applications-system-symbolic</property>
        <property name="needs-attention">True</property>
        <property name="badge-number">3</property>
        <property name="child">
          <object class="AdwBin"/>
        </property>
      </object>
    </child>
    <child>
      <object class="AdwViewStackPage">
        <property name="title">Page 5</property>
        <property name="icon-name">applications-system-symbolic</property>
        <property name="starts-section">True</property>
        <property name="child">
          <object class="AdwBin"/>
        </property>
      </object>
    </child>
    <child>
      <object class="AdwViewStackPage">
        <property name="title">Page 6</property>
        <property name="icon-name">applications-system-symbolic</property>
        <property name="needs-attention">True</property>
        <property name="child">
          <object class="AdwBin"/>
        </property>
      </object>
    </child>
  </object>
</interface>
